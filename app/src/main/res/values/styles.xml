<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <!-- Customize your theme here. --><!--
        <item name="colorPrimary">@color/Primary</item>-->
    </style>

    <style name="AppTheme.NoActionBar" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item><!--
        <item name="colorPrimary">@color/Primary</item>-->
    </style>

    <style name="MyButton" parent="Theme.AppCompat.Light"><!--
        <item name="colorControlHighlight">#a9006e</item>-->
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" >
        <item name="colorPrimary">@color/Primary</item><!--
        <item name="android:textColorPrimary">@color/textPrimary</item>-->
    </style>

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" ><!--
        <item name="android:textColor">@color/material_drawer_dark_selected_text</item>-->
    </style>

    <style name="menu_labels_style">
        <item name="android:background">@drawable/fab_label_background</item>
        <item name="android:textColor">@color/white</item>
    </style>

    <style name="leave_data_dialog_text_content">
        <item name="android:text">center_horizontal</item>
    </style>
    <style name="Dialog.NoTitle" parent="Theme.AppCompat.Light.Dialog">
        <item name="android:windowNoTitle">true</item><!--
        <item name="android:padding">0dp</item>
        <item name="android:windowBackground">@color/black_semi_transparent</item>-->
    </style>


</resources>
